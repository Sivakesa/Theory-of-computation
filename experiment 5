#include <stdio.h>
#include <string.h>

int S(char*);
int A(char*);

int main() {
    char input[100];
    printf("Enter a string: ");
    scanf("%s", input);
    if (S(input) == 1 && strlen(input) == 0) {
        printf("Accepted\n");
    } else {
        printf("Rejected\n");
    }
    return 0;
}

int S(char* input) {
    if (input[0] == '0') {
        return S(&input[1]);
    } else {
        return A(input);
    }
}

int A(char* input) {
    if (input[0] == '1') {
        return A(&input[1]);
    } else if (input[0] == '\0') {
        return 1;
    } else {
        return 0;
    }
}
